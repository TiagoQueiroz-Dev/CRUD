{
  "include": [
      "C:/estudos/projeto/bACK/core/domain/Entities/*.cs",
      "C:/estudos/projeto/bACK/core/application/ViewModels/*/*.cs",
      "C:/estudos/projeto/bACK/core/domain/Entities/EntidadeBase/*.cs"
